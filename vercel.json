{
    "version": 2,
    "builds": [
      {
        "src": "./package.json",
        "use": "@vercel/react"
      },
      {
        "src": "backend/app.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/currency-exchange/(.*)",
        "dest": "/backend/index.js"
      },
      {
        "src": "/(.*)",
        "dest": "/build/$1"
      }
    ]
  }
  